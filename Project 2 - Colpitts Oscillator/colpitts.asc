TEXT 824 -216 Left 0 !* Window\n.param TIME1 = 13m\n.param TIME2 = 15m\n \n* Peak-to-Peak Voltage\n.meas tran Vout_pp PARAM Vout_max - Vout_min\n.meas tran Vin_pp PARAM Vin_max - Vin_min\n \n*Gain\n.meas tran Gain PARAM Vout_pp / Vin_pp\n \n* Power Consumption      \n.meas tran Average_Power AVG (V(Vcc)*Iavg) FROM {TIME1} TO {TIME2}\n.meas tran Max_Power MAX (V(Vcc)*Imax) FROM {TIME1} TO {TIME2}\n \n* VBE, VCE, IC\n.meas tran VBE AVG (V(Vb)-V(Ve)) FROM {TIME1} TO {TIME2}\n.meas tran VCE AVG (V(Vc)-V(Ve)) FROM {TIME1} TO {TIME2}\n.meas tran Ic AVG abs(I(RC)) FROM {TIME1} TO {TIME2}   \n \n* Transconductance (at room temp: VT = 26 mV)\n.meas tran gm PARAM Ic/0.026\n \n* Other\n.meas tran Vout_max MAX V(Vout) FROM {TIME1} TO {TIME2}\n.meas tran Vout_min MIN V(Vout) FROM {TIME1} TO {TIME2}\n.meas tran Vin_max MAX V(3) FROM {TIME1} TO {TIME2}\n.meas tran Vin_min MIN V(3) FROM {TIME1} TO {TIME2}\n.meas tran Iavg AVG (-I(10V)) FROM {TIME1} TO {TIME2}\n.meas tran Imax MAX (-I(10V)) FROM {TIME1} TO {TIME2}
